.rscontainer{
    width: 75%;
    min-height:460px;
    border: 1px solid #e9e9e9;
    background-color: #f9f9f9;

    float: left;
    padding: 40px 30px;
    position:relative;
}

.rsmenu{
    width: 20%;
    min-height:460px;
    border: 1px solid #e9e9e9;
    background-color: #f9f9f9;
    float: left;
}
.user-task{
    padding: 16px;
    position: absolute;
    z-index: 4;
    border: 1px solid #2e6f9a;
    border-radius: 8px;
    opacity: 0.8;
    cursor: move;
    background-color: white;
    font-size: 11px;
    width:100px;
    height:85px;
}
.start-task{
    padding: 16px;
    position: absolute;
    z-index: 4;
    border: 1px solid #2e6f9a;
    border-radius: 8px;
    opacity: 0.8;
    cursor: move;
    background-color: white;
    font-size: 11px;
    width:80px;
    height:80px;
    border-radius: 40px;
}
.end-task{
    padding: 16px;
    position: absolute;
    z-index: 4;
    border: 3px solid #000;
    border-radius: 8px;
    opacity: 0.8;
    cursor: move;
    background-color: white;
    font-size: 11px;
    width:80px;
    height:80px;
    border-radius: 40px;
}
.menu-task{
    position: relative;
    margin: 10px 10px;
}
.rs-line{
    /*width:80px;*/
    height:6px;
    border: 3px solid #000;
    z-index: 4;
    opacity: 0.8;
    cursor: move;
    position:absolute;
}
.rs-arrow-right{
    /*background-image: url("../images/arrow_right.png");*/
    position:absolute;
